{
  "name": "@singee-templates/fullstack",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.12.2",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "node .output/server/index.mjs",
    "test": "vitest run",
    "test:unit": "vitest run --project unit",
    "test:browser": "vitest run --project browser",
    "lint": "eslint",
    "format": "prettier --write . && eslint --fix && eslint",
    "check:types": "tsc --noEmit",
    "migrations:generate": "drizzle-kit generate",
    "migrations:apply": "drizzle-kit migrate",
    "migrations:push": "drizzle-kit push",
    "auth:generate": "npx @better-auth/cli generate --config ./src/auth/server.ts --output ./src/db/auth-schema.ts -y && pnpm format",
    "db:seed": "tsx scripts/seed-db.ts",
    "drizzle:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@mantine/carousel": "^8.3.14",
    "@mantine/charts": "^8.3.14",
    "@mantine/code-highlight": "^8.3.14",
    "@mantine/core": "^8.3.14",
    "@mantine/dates": "^8.3.14",
    "@mantine/dropzone": "^8.3.14",
    "@mantine/form": "^8.3.14",
    "@mantine/hooks": "^8.3.14",
    "@mantine/modals": "^8.3.14",
    "@mantine/nprogress": "^8.3.14",
    "@mantine/spotlight": "^8.3.14",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.159.5",
    "@tanstack/react-router-ssr-query": "^1.159.5",
    "@tanstack/react-start": "^1.159.5",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-plugin": "^1.159.5",
    "better-auth": "^1.4.18",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.19",
    "drizzle-orm": "^0.45.1",
    "postgres": "^3.4.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sonner": "^2.0.7",
    "ulid": "^3.0.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.18",
    "@tanstack/devtools-vite": "^0.5.1",
    "@tanstack/eslint-config": "^0.3.4",
    "@tanstack/react-devtools": "^0.9.5",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router-devtools": "^1.159.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^24.10.12",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "lefthook": "^2.1.0",
    "nitro": "3.0.1-alpha.1",
    "playwright": "^1.58.2",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.0",
    "vitest": "^4.0.18",
    "vitest-browser-react": "^2.0.5",
    "web-vitals": "^5.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "eslint",
      "lefthook",
      "playwright"
    ],
    "overrides": {
      "drizzle-orm": "$drizzle-orm"
    }
  }
}
